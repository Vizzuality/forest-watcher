{
  "name": "ForestWatcher",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged"
  },
  "dependencies": {
    "@mapbox/geojson-area": "^0.2.2",
    "@mapbox/tilebelt": "^1.0.1",
    "boundingbox": "^0.1.0",
    "buffer": "^5.0.5",
    "geojson-polygon-self-intersections": "^1.1.1",
    "geopoint": "^1.0.1",
    "p-map": "^1.1.1",
    "react": "15.4.2",
    "react-native": "0.42.3",
    "react-native-camera": "0.6.0",
    "react-native-config": "0.3.1",
    "react-native-datepicker": "1.4.5",
    "react-native-fetch-blob": "0.10.4",
    "react-native-google-analytics-bridge": "5.0.0",
    "react-native-gps": "Vizzuality/react-native-gps",
    "react-native-i18n": "1.0.0",
    "react-native-keyboard-spacer": "0.3.1",
    "react-native-location": "timfpark/react-native-location",
    "react-native-maps": "Vizzuality/react-native-maps#feature/canvas-android",
    "react-native-scrollable-tab-view": "0.7.3",
    "react-native-sensor-manager": "0.1.10",
    "react-navigation": "1.0.0-beta.7",
    "react-redux": "5.0.2",
    "redux": "^3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-form": "6.6.1",
    "redux-persist": "4.6.0",
    "redux-thunk": "2.2.0",
    "turf-intersect": "Vizzuality/turf-intersect"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-jest": "17.0.2",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-react-native": "1.9.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-import-resolver-babel-module": "^2.2.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "husky": "^0.13.3",
    "jest": "17.0.3",
    "lint-staged": "^3.4.0",
    "react-test-renderer": "15.4.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "babel": {
    "presets": [
      "react-native"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "app": "./app",
            "routes": "./app/routes",
            "locales": "./app/locales",
            "config": "./app/config",
            "helpers": "./app/helpers",
            "components": "./app/components",
            "containers": "./app/containers",
            "redux-modules": "./app/redux-modules",
            "assets": "./app/assets"
          }
        }
      ]
    ]
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  }
}
