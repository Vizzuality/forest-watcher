{
  "name": "ForestWatcher",
  "version": "1.6.3",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "npm start -- --reset-cache",
    "start:clear": "npm run clear && npm run start:reset",
    "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all",
    "clear:modules": "rm -rf node_modules",
    "clear": "npm run clear:cache && npm run clear:modules && npm install",
    "precommit": "lint-staged",
    "transifex:extract": "i18next-scanner --config ./app/locales/extract.config.js './app/**/*.{js,jsx}'",
    "transifex:push": "node transifex/transifex-push.js",
    "transifex:pull": "node transifex/transifex-pull.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release:android": "cd android && ./gradlew assembleRelease",
    "release:ios": "react-native run-ios --configuration Release",
    "postinstall:ios": "cd node_modules/react-native; exec ./scripts/ios-install-third-party.sh; cd third-party/glog-0.3.4; exec ./configure; cd ../../../.."
  },
  "dependencies": {
    "@mapbox/geo-viewport": "0.4.0",
    "@mapbox/geojson-area": "0.2.2",
    "@redux-offline/redux-offline": "2.5.1",
    "appcenter": "1.10.0",
    "boundingbox": "0.1.2",
    "buffer": "5.2.1",
    "d3-dsv": "1.0.10",
    "formatcoords": "1.1.3",
    "geojson-polygon-self-intersections": "1.2.0",
    "geokdbush": "1.1.0",
    "jsonapi-serializer": "3.6.4",
    "kdbush": "1.0.1",
    "lodash": "4.17.11",
    "lottie-ios": "2.5.0",
    "lottie-react-native": "2.5.10",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-app-auth": "2.6.1",
    "react-native-code-push": "5.4.2",
    "react-native-config": "0.11.6",
    "react-native-cookies": "3.3.0",
    "react-native-datepicker": "1.7.2",
    "react-native-fast-image": "5.1.1",
    "react-native-fbsdk": "0.8.0",
    "react-native-fetch-blob": "0.10.8",
    "react-native-google-analytics-bridge": "5.3.0",
    "react-native-hyperlink": "0.0.14",
    "react-native-i18n": "2.0.15",
    "react-native-image-picker": "0.27.1",
    "react-native-keyboard-aware-scroll-view": "0.3.0",
    "react-native-location": "github:timfpark/react-native-location",
    "react-native-maps": "0.22.1",
    "react-native-navigation": "1.1.486",
    "react-native-progress": "3.5.0",
    "react-native-scrollable-tab-view": "0.10.0",
    "react-native-snap-carousel": "3.7.5",
    "react-native-timer": "1.3.4",
    "react-native-zip-archive": "3.0.1",
    "react-redux": "5.1.1",
    "realm": "1.13.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.6",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "supercluster": "4.1.1",
    "utm-latlng": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "3.1.1",
    "code-push-cli": "2.1.9",
    "dotenv": "6.1.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-babel-module": "4.0.0",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.78.0",
    "i18next-scanner": "2.7.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "metro-react-native-babel-preset": "0.47.1",
    "node-fetch": "2.3.0",
    "react-test-renderer": "16.6.1",
    "reactotron-react-native": "2.1.0",
    "reactotron-redux": "2.1.0",
    "reactotron-redux-saga": "2.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "app/**/*.js": [
      "eslint"
    ]
  }
}
