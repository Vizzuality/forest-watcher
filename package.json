{
    "name": "ForestWatcher",
    "version": "1.6.3",
    "private": true,
    "scripts": {
        "start": "node node_modules/react-native/local-cli/cli.js start",
        "start:reset": "npm start -- --reset-cache",
        "start:clear": "npm run clear && npm run start:reset",
        "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all",
        "clear:modules": "rm -rf node_modules",
        "clear": "npm run clear:cache && npm run clear:modules && npm install",
        "precommit": "lint-staged",
        "transifex:extract": "i18next-scanner --config ./app/locales/extract.config.js './app/**/*.{js,jsx}'",
        "transifex:push": "node transifex/transifex-push.js",
        "transifex:pull": "node transifex/transifex-pull.js",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "release:android": "cd android && ./gradlew assembleRelease",
        "release:ios": "react-native run-ios --configuration Release",
        "postinstall:ios": "cd node_modules/react-native; exec ./scripts/ios-install-third-party.sh; cd third-party/glog-0.3.4; exec ./configure; cd ../../../..",
        "lint": "node_modules/.bin/eslint ./app/"
    },
    "dependencies": {
        "@mapbox/geo-viewport": "0.2.2",
        "@mapbox/geojson-area": "0.2.2",
        "@mapbox/tilebelt": "github:Vizzuality/tilebelt",
        "@redux-offline/redux-offline": "2.3.3",
        "appcenter": "1.4.0",
        "appcenter-crashes": "1.4.0",
        "boundingbox": "0.1.0",
        "buffer": "5.0.5",
        "d3-dsv": "1.0.5",
        "formatcoords": "1.1.3",
        "geojson-polygon-self-intersections": "1.1.1",
        "geokdbush": "1.1.0",
        "geopoint": "1.0.1",
        "jsonapi-serializer": "3.5.3",
        "kdbush": "1.0.1",
        "lodash": "4.17.4",
        "lottie-ios": "2.5.0",
        "lottie-react-native": "2.5.0",
        "moment": "2.22.0",
        "prop-types": "15.5.10",
        "react": "16.3.1",
        "react-native": "0.55.2",
        "react-native-app-auth": "2.4.0",
        "react-native-code-push": "5.3.2",
        "react-native-config": "0.11.5",
        "react-native-cookies": "3.2.0",
        "react-native-datepicker": "1.6.0",
        "react-native-fast-image": "4.0.14",
        "react-native-fbsdk": "0.8.0",
        "react-native-fetch-blob": "0.10.8",
        "react-native-google-analytics-bridge": "5.3.0",
        "react-native-gps": "github:Vizzuality/react-native-gps",
        "react-native-hyperlink": "0.0.11",
        "react-native-i18n": "2.0.7",
        "react-native-image-picker": "0.26.7",
        "react-native-keyboard-aware-scroll-view": "0.3.0",
        "react-native-location": "github:timfpark/react-native-location",
        "react-native-maps": "github:Vizzuality/react-native-maps",
        "react-native-navigation": "1.1.434",
        "react-native-progress": "3.4.0",
        "react-native-scrollable-tab-view": "0.8.0",
        "react-native-sensor-manager": "0.1.10",
        "react-native-snap-carousel": "3.4.0",
        "react-native-timer": "1.3.1",
        "react-native-zip-archive": "2.1.0",
        "react-redux": "5.0.6",
        "realm": "1.13.0",
        "redux": "3.6.0",
        "redux-devtools-extension": "2.13.0",
        "redux-saga": "0.16.0",
        "redux-thunk": "2.2.0",
        "sphere-knn": "1.3.1",
        "supercluster": "^3.0.2",
        "turf-intersect": "github:Vizzuality/turf-intersect",
        "utm-latlng": "1.0.5"
    },
    "devDependencies": {
        "babel-core": "6.26.3",
        "babel-eslint": "10.0.1",
        "babel-jest": "17.0.2",
        "babel-plugin-module-resolver": "3.1.1",
        "babel-preset-flow": "6.23.0",
        "babel-preset-react-native": "4.0.1",
        "code-push-cli": "2.1.6",
        "dotenv": "4.0.0",
        "eslint": "5.8.0",
        "eslint-config-prettier": "3.0.1",
        "eslint-import-resolver-babel-module": "4.0.0",
        "eslint-plugin-flowtype": "3.2.0",
        "eslint-plugin-import": "2.14.0",
        "eslint-plugin-prettier": "2.6.2",
        "eslint-plugin-react": "7.11.1",
        "eslint-plugin-react-native": "3.5.0",
        "flow-bin": "0.65.0",
        "husky": "0.13.3",
        "i18next-scanner": "2.4.6",
        "jest": "17.0.3",
        "lint-staged": "3.4.0",
        "node-fetch": "1.7.1",
        "prettier": "1.14.2",
        "react-test-renderer": "15.4.1",
        "reactotron-react-native": "2.0.0-beta.4",
        "reactotron-redux": "1.13.0",
        "reactotron-redux-saga": "1.13.0"
    },
    "jest": {
        "preset": "react-native"
    },
    "prettier": {
        "arrowParens": "avoid",
        "bracketSpacing": true,
        "jsxBracketSameLine": false,
        "printWidth": 140,
        "semi": true,
        "singleQuote": true,
        "tabWidth": 2,
        "trailingComma": "none",
        "useTabs": false
    },
    "lint-staged": {
        "app/**/*.js": [
            "eslint"
        ]
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "commonjs": true,
            "es6": true,
            "node": true,
            "react-native/react-native": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:flowtype/recommended",
            "plugin:import/errors",
            "plugin:prettier/recommended",
            "plugin:react/all",
            "plugin:react-native/all",
            "prettier/react"
        ],
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 2017,
            "ecmaFeatures": {
                "experimentalObjectRestSpread": true,
                "jsx": true
            },
            "sourceType": "module"
        },
        "plugins": [
            "flowtype",
            "prettier",
            "react",
            "react-native"
        ],
        "rules": {
            "no-console": [
                "error",
                {
                    "allow": [
                        "warn",
                        "error",
                        "info"
                    ]
                }
            ],
            "no-unused-vars": "warn",
            "prettier/prettier": "off",
            "react/default-props-match-prop-types": [
                "warn",
                {
                    "allowRequiredDefaults": true
                }
            ],
            "react/destructuring-assignment": "off",
            "react/forbid-component-props": "off",
            "react/forbid-prop-types": "off",
            "react/jsx-boolean-value": [
                "off",
                "always"
            ],
            "react/jsx-curly-brace-presence": "off",
            "react/jsx-handler-names": "off",
            "react/jsx-indent": "off",
            "react/jsx-filename-extension": "off",
            "react/jsx-max-depth": "off",
            "react/jsx-max-props-per-line": "off",
            "react/jsx-no-bind": "off",
            "react/jsx-no-literals": "off",
            "react/jsx-sort-default-props": "warn",
            "react/jsx-sort-props": "off",
            "react/no-array-index-key": "warn",
            "react/no-set-state": "off",
            "react/prefer-stateless-function": "off",
            "react/prop-types": "warn",
            "react/require-default-props": "off",
            "react/require-optimization": "off",
            "react/sort-comp": "off",
            "react/sort-prop-types": "off",
            "react-native/no-color-literals": "off",
            "react-native/no-inline-styles": "off",
            "react-native/split-platform-components": "off"
        },
        "settings": {
            "import/resolver": {
                "babel-module": {},
                "node": {
                    "extensions": [
                        ".js",
                        ".jsx"
                    ],
                    "moduleDirectory": [
                        "node_modules",
                        "app"
                    ]
                }
            },
            "flowtype": {
                "onlyFilesWithFlowAnnotation": true
            }
        }
    }
}
