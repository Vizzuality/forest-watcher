{
  "name": "ForestWatcher",
  "version": "1.7.2",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:reset": "npm start -- --reset-cache",
    "start:clear": "npm run clear && npm run start:reset",
    "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all",
    "clear:modules": "rm -rf node_modules",
    "clear": "npm run clear:cache && npm run clear:modules && npm install",
    "precommit": "lint-staged",
    "transifex:extract": "i18next-scanner --config ./app/locales/extract.config.js './app/**/*.{js,jsx}'",
    "transifex:push": "node transifex/transifex-push.js",
    "transifex:pull": "node transifex/transifex-pull.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "release:android": "cd android && ./gradlew assembleRelease",
    "release:ios": "react-native run-ios --configuration Release",
    "postinstall:ios": "cd node_modules/react-native; exec ./scripts/ios-install-third-party.sh; cd third-party/glog-0.3.4; exec ./configure; cd ../../../..",
    "test": "jest --forceExit --detectOpenHandles",
    "test:ci": "jest --forceExit --detectOpenHandles --ci --coverage",
    "test:watch": "jest --watch",
    "lint": "node_modules/.bin/eslint ./app/",
    "lint:ci": "node_modules/.bin/eslint ./app/ -o reports/lint.html -f html",
    "codepush:release:android": "code-push release-react forest-watcher-android-client android -k private.pem",
    "codepush:release:ios": "code-push release-react forest-watcher-ios-client ios -k private.pem"
  },
  "dependencies": {
    "@mapbox/geo-viewport": "0.4.0",
    "@mapbox/geojson-area": "0.2.2",
    "@redux-offline/redux-offline": "2.5.1",
    "boundingbox": "0.1.2",
    "buffer": "5.2.1",
    "d3-dsv": "1.0.10",
    "formatcoords": "1.1.3",
    "geojson-polygon-self-intersections": "1.2.0",
    "geokdbush": "1.1.0",
    "json2csv": "4.4.0",
    "jsonapi-serializer": "3.6.4",
    "kdbush": "1.0.1",
    "lodash": "4.17.11",
    "lottie-ios": "2.5.0",
    "lottie-react-native": "2.5.10",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.6.1",
    "react-native": "0.57.5",
    "react-native-app-auth": "2.6.1",
    "react-native-code-push": "5.5.0",
    "react-native-config": "0.11.6",
    "react-native-cookies": "3.3.0",
    "react-native-datepicker": "1.7.2",
    "react-native-dialogs": "1.0.4",
    "react-native-fast-image": "5.1.1",
    "react-native-fbsdk": "0.8.0",
    "react-native-fetch-blob": "0.10.8",
    "react-native-google-analytics-bridge": "5.3.0",
    "react-native-hyperlink": "0.0.14",
    "react-native-i18n": "2.0.15",
    "react-native-image-picker": "0.27.2",
    "react-native-keyboard-aware-scroll-view": "0.3.0",
    "react-native-location": "2.2.0",
    "react-native-maps": "github:3sidedcube/react-native-maps#55f879c5071eb4deb76f605d5d87b0ac852e9d9f",
    "react-native-navigation": "2.1.3",
    "react-native-progress": "3.5.0",
    "react-native-safe-area": "0.5.0",
    "react-native-scrollable-tab-view": "0.10.0",
    "react-native-sentry": "0.40.2",
    "react-native-snap-carousel": "3.7.5",
    "react-native-timer": "1.3.4",
    "react-native-webview": "2.15.0",
    "react-native-zip-archive": "3.0.1",
    "react-redux": "5.1.1",
    "realm": "1.13.0",
    "redux": "4.0.1",
    "redux-devtools-extension": "2.13.6",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "supercluster": "4.1.1",
    "url-parse": "1.4.4",
    "utm-latlng": "1.0.5"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.4.2",
    "babel-plugin-module-resolver": "3.1.1",
    "code-push-cli": "2.1.9",
    "dotenv": "6.1.0",
    "eslint": "5.9.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-plugin-react-native": "3.5.0",
    "flow-bin": "0.78.0",
    "i18next-scanner": "2.7.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "1.7.4",
    "jest-html-reporter": "2.4.2",
    "lint-staged": "8.1.0",
    "metro-react-native-babel-preset": "0.47.1",
    "node-fetch": "2.3.0",
    "prettier": "1.15.2",
    "react-test-renderer": "16.6.1",
    "reactotron-react-native": "2.1.0",
    "reactotron-redux": "2.1.0",
    "reactotron-redux-saga": "2.1.0",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "coverageDirectory": "reports/coverage",
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "outputPath": "reports/test-report.html"
        }
      ]
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "lint-staged": {
    "app/**/*.js": [
      "eslint"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true,
      "react-native/react-native": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:flowtype/recommended",
      "plugin:import/errors",
      "plugin:prettier/recommended",
      "plugin:react/all",
      "plugin:react-native/all",
      "prettier/react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "flowtype",
      "prettier",
      "react",
      "react-native"
    ],
    "rules": {
      "no-console": [
        "error",
        {
          "allow": [
            "warn",
            "error",
            "info"
          ]
        }
      ],
      "no-unused-vars": [
        "warn",
        {
          "args": "none"
        }
      ],
      "prettier/prettier": "warn",
      "react/default-props-match-prop-types": [
        "warn",
        {
          "allowRequiredDefaults": true
        }
      ],
      "react/destructuring-assignment": "off",
      "react/display-name": "warn",
      "react/forbid-component-props": "off",
      "react/forbid-prop-types": "off",
      "react/jsx-boolean-value": [
        "off",
        "always"
      ],
      "react/jsx-curly-brace-presence": "off",
      "react/jsx-handler-names": "off",
      "react/jsx-indent": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-max-depth": "off",
      "react/jsx-max-props-per-line": "off",
      "react/jsx-no-bind": "off",
      "react/jsx-no-literals": "off",
      "react/jsx-sort-default-props": "warn",
      "react/jsx-sort-props": "off",
      "react/no-array-index-key": "off",
      "react/no-set-state": "off",
      "react/no-unsafe": "warn",
      "react/prefer-stateless-function": "off",
      "react/prop-types": "warn",
      "react/require-default-props": "off",
      "react/require-optimization": "off",
      "react/sort-comp": "off",
      "react/sort-prop-types": "off",
      "react-native/no-color-literals": "off",
      "react-native/no-inline-styles": "off",
      "react-native/split-platform-components": "off"
    },
    "settings": {
      "import/resolver": {
        "babel-module": {},
        "node": {
          "extensions": [
            ".js",
            ".jsx"
          ],
          "moduleDirectory": [
            "node_modules",
            "app"
          ]
        }
      },
      "flowtype": {
        "onlyFilesWithFlowAnnotation": true
      }
    }
  }
}
