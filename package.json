{
  "name": "ForestWatcher",
  "version": "1.0.0.rc5",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "eslint .",
    "test": "jest",
    "lint-staged": "lint-staged",
    "precommit": "npm run lint-staged",
    "transifex:push": "node transifex/transifex-push.js",
    "transifex:pull": "node transifex/transifex-pull.js",
    "preinstall": "source no-package-lock.sh"
  },
  "dependencies": {
    "@mapbox/geo-viewport": "0.2.2",
    "@mapbox/geojson-area": "0.2.2",
    "@mapbox/tilebelt": "Vizzuality/tilebelt",
    "boundingbox": "0.1.0",
    "buffer": "5.0.5",
    "d3-dsv": "1.0.5",
    "geojson-polygon-self-intersections": "1.1.1",
    "geokdbush": "1.1.0",
    "geopoint": "1.0.1",
    "jsonapi-serializer": "3.5.3",
    "kdbush": "1.0.1",
    "lodash": "4.17.4",
    "moment": "2.18.1",
    "prop-types": "15.5.10",
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.1",
    "react-native-config": "0.3.1",
    "react-native-cookies": "3.1.0",
    "react-native-datepicker": "1.4.5",
    "react-native-fetch-blob": "0.10.4",
    "react-native-google-analytics-bridge": "5.0.0",
    "react-native-google-signin": "0.10.0",
    "react-native-gps": "Vizzuality/react-native-gps",
    "react-native-i18n": "1.0.0",
    "react-native-image-picker": "^0.26.3",
    "react-native-keyboard-spacer": "0.3.1",
    "react-native-location": "timfpark/react-native-location",
    "react-native-maps": "Vizzuality/react-native-maps",
    "react-native-navigation": "1.1.65",
    "react-native-progress": "^3.3.0",
    "react-native-scrollable-tab-view": "0.7.4",
    "react-native-sensor-manager": "0.1.10",
    "react-native-snap-carousel": "2.1.2",
    "react-native-timer": "1.3.1",
    "react-native-zip-archive": "2.0.0",
    "react-redux": "5.0.2",
    "realm": "1.10.0",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-form": "6.6.1",
    "redux-offline": "github:sorodrigo/redux-offline",
    "redux-persist": "4.6.0",
    "redux-thunk": "2.2.0",
    "sphere-knn": "1.3.1",
    "supercluster": "2.3.0",
    "turf-intersect": "Vizzuality/turf-intersect"
  },
  "devDependencies": {
    "babel-eslint": "7.1.1",
    "babel-jest": "17.0.2",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-preset-react-native": "1.9.0",
    "dotenv": "4.0.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-import-resolver-babel-module": "2.2.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "husky": "0.13.3",
    "jest": "17.0.3",
    "lint-staged": "3.4.0",
    "node-fetch": "1.7.1",
    "react-test-renderer": "15.4.1",
    "reactotron-react-native": "1.11.0",
    "reactotron-redux": "1.11.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "babel": {
    "presets": [
      "react-native"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "app": "./app",
            "screens": "./app/screens",
            "locales": "./app/locales",
            "config": "./app/config",
            "helpers": "./app/helpers",
            "components": "./app/components",
            "containers": "./app/containers",
            "redux-modules": "./app/redux-modules",
            "assets": "./app/assets",
            "offline": "./app/offline",
            "package.json": "./package.json"
          }
        }
      ]
    ]
  },
  "lint-staged": {
    "app/**/*.js": "eslint"
  }
}
